<%- include ../layout/header %>
<div class="fix auto w980">
	<%- include ../layout/nav %>
	<div class="cell">
		<% for(var i = 0 ;i <postlist.length;i++){%>
		<div class="bbd">
			<h2 class="f20 p5 red"><%=postlist[i].title%> <span class="f12 gray"><%=postlist[i].time.ymdm%></span></h2>
			<h2 class="f14 p5"><%=postlist[i].content%></h2>
		</div>
		<% }%>
	</div>
</div>
<script src="../../js/jq.js"></script>
<script src="../../js/hValidate.js"></script>
<script>
	formValidate("registerForm","/users/register",function(k){
		var json = k;
		if (json.error){
			alert(json.msg);
		}else{
			alert(json.msg);
			window.location.href=json.href;
		}
		
	});
</script>
